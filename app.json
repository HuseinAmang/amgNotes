{
  "expo": {
    "name": "amgNotes",
    "slug": "amgNotes",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/ic_launcher.png",
    "splash": {
      "image": "./app/assets/amgNotes.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFEB3B"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.amg.amgnotes",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/ic_launcher.png",
        "backgroundColor": "#FFEB3B"
      }
    },
    "web": {
      "favicon": "./assets/ic_launcher.png"
    }
  }
}
